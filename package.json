{
  "name": "simonheys",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -k -r \"yarn generate:watch\" \"yarn next\"",
    "build": "yarn generate && next build",
    "start": "next start",
    "lint": "next lint",
    "generate": "yarn generate:content && yarn generate:image-properties && yarn generate:icon-components",
    "generate:content": "ts-node ./src/scripts/generate-content",
    "generate:favicon": "ts-node ./src/scripts/generate-favicon",
    "generate:icon-components": "ts-node ./src/scripts/generate-icon-components",
    "generate:image-properties": "ts-node ./src/scripts/generate-image-properties",
    "generate:watch": "concurrently -k -r \"yarn generate:content --watch\" \"yarn generate:icon-components --watch\" \"yarn generate:image-properties --watch\"",
    "prettier": "prettier --write \"./**/*.{js,json,css,scss,yml}\"",
    "test": "start-server-and-test dev http://localhost:3000 cypress",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "e2e": "start-server-and-test start http://localhost:3000 cypress",
    "e2e:headless": "start-server-and-test start http://localhost:3000 cypress:headless"
  },
