{
  "name": "simonheys",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -k -r \"pnpm generate:watch\" \"pnpm next\"",
    "build": "pnpm generate && next build",
    "start": "next start",
    "lint": "eslint .",
    "generate": "pnpm generate:content && pnpm generate:image-properties && pnpm generate:icon-components",
    "generate:content": "tsx ./src/scripts/generate-content",
    "generate:favicon": "tsx ./src/scripts/generate-favicon",
    "generate:icon-components": "tsx ./src/scripts/generate-icon-components",
    "generate:image-properties": "tsx ./src/scripts/generate-image-properties",
    "generate:watch": "concurrently -k -r \"pnpm generate:content --watch\" \"pnpm generate:icon-components --watch\" \"pnpm generate:image-properties --watch\"",
    "prettier": "prettier --write \"./**/*.{js,json,css,scss,yml,ts,tsx}\"",
    "test": "jest",
    "test:e2e": "playwright test",
    "prepare": "husky install"
  },
  "dependencies": {
    "@resvg/resvg-js": "^2.4.1",
    "@simonheys/wordclock": "^1.1.6",
    "@simonheys/wordclock-words": "^1.1.6",
    "@vercel/analytics": "^1.0.1",
    "@vimeo/player": "^2.19.0",
    "bootstrap": "^5.3.2",
    "clsx": "^2.0.0",
    "colord": "^2.9.3",
    "favicons": "^7.1.3",
    "feed": "^4.2.2",
    "framer-motion": "^10.12.18",
    "graphology": "^0.25.1",
    "graphology-layout-forceatlas2": "^0.10.1",
    "graphology-utils": "^2.5.2",
    "next": "^14.0.0",
    "popmotion": "^11.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intersection-observer": "^9.5.2",
    "react-markdown": "^9.0.0",
    "resize-observer-polyfill": "^1.5.1",
    "sass": "^1.62.1",
    "swr": "^2.2.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.36.1",
    "@svgr/core": "^8.0.0",
    "@svgr/plugin-jsx": "^8.0.1",
    "@svgr/plugin-prettier": "^8.0.1",
    "@svgr/plugin-svgo": "^8.0.1",
    "@testing-library/jest-dom": "6.4.5",
    "@testing-library/react": "14.3.1",
    "@testing-library/user-event": "14.5.2",
    "@tsconfig/node16": "^16.1.0",
    "@types/cli-progress": "^3.11.0",
    "@types/fs-extra": "^11.0.1",
    "@types/get-pixels": "^3.3.2",
    "@types/jest": "^29.5.3",
    "@types/js-yaml": "^4.0.5",
    "@types/ndarray": "^1.0.11",
    "@types/node": "^20.4.2",
    "@types/react": "^18.2.14",
    "@types/resize-observer-browser": "^0.1.7",
    "@types/vimeo__player": "^2.18.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "chokidar": "^3.5.3",
    "cli-progress": "^3.12.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.42.0",
    "eslint-config-next": "^14.0.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^28.0.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.0.8",
    "eslint-plugin-testing-library": "^6.0.1",
    "fs-extra": "^11.1.1",
    "get-pixels": "^3.3.3",
    "graphology-types": "^0.24.7",
    "husky": "^8.0.3",
    "image-size": "^1.0.2",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "29.7.0",
    "js-yaml": "^4.1.0",
    "minimist": "^1.2.8",
    "prettier": "^3.0.1",
    "rimraf": "^5.0.1",
    "tsx": "^4.9.3",
    "typescript": "^5.1.6"
  },
  "main": "index.js",
  "repository": "git@github.com:simonheys/simonheys.com.git",
  "author": "Simon Heys <si@simonheys.com>",
  "license": "MIT",
  "lint-staged": {
    "*": "prettier --ignore-unknown --write",
    "*.{js,jsx,ts,tsx}": "eslint --fix"
  }
}
