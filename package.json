{
  "name": "simonheys",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn clean && concurrently -k -r \"yarn generate:watch\" \"yarn next\"",
    "build": "yarn generate && next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "rimraf .next",
    "generate": "yarn generate:content && yarn generate:image-properties && yarn generate:icon-components",
    "generate:content": "node ./src/scripts/generate-content",
    "generate:favicon": "node ./src/scripts/generate-favicon",
    "generate:icon-components": "node ./src/scripts/generate-icon-components",
    "generate:image-properties": "node ./src/scripts/generate-image-properties",
    "generate:watch": "concurrently -k -r \"yarn generate:content --watch\" \"yarn generate:icon-components --watch\" \"yarn generate:image-properties --watch\"",
    "prettier": "prettier --write \"./**/*.{js,json,css,scss}\""
  },
  "dependencies": {
    "@vimeo/player": "^2.16.1",
    "bootstrap": "^5.1.0",
    "clsx": "^1.1.1",
    "color": "^4.0.1",
    "favicons": "^6.2.2",
    "framer-motion": "^4.1.17",
    "graphology": "^0.20.0",
    "graphology-layout-forceatlas2": "^0.6.1",
    "next": "11.1.2",
    "popmotion": "^9.4.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-intersection-observer": "^8.32.0",
    "react-markdown": "^6.0.3",
    "resize-observer-polyfill": "^1.5.1",
    "sass": "^1.35.2",
    "swr": "^0.5.6",
    "wordclock": "https://github.com/simonheys/wordclock.git#v1.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/node": "^7.14.9",
    "@svgr/core": "^5.5.0",
    "@svgr/plugin-jsx": "^5.5.0",
    "@svgr/plugin-prettier": "^5.5.0",
    "@svgr/plugin-svgo": "^5.5.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^4.0.0",
    "babel-eslint": "^10.0.0",
    "chokidar": "^3.5.2",
    "cli-progress": "^3.9.0",
    "concurrently": "^6.2.1",
    "eslint": "^7.5.0",
    "eslint-config-next": "11.0.1",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "eslint-plugin-testing-library": "^4.10.1",
    "get-pixels": "^3.3.3",
    "image-size": "^1.0.0",
    "js-yaml": "^4.1.0",
    "minimist": "^1.2.5",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "sharp": "^0.28.3",
    "typescript": "^4.4.3"
  }
}
